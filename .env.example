# Anthropic API Key
# Получить: https://console.anthropic.com/settings/keys
# Используется для доступа к Claude Sonnet 4.5
ANTHROPIC_API_KEY=sk-ant-api03-your_anthropic_api_key_here

# Brave Search API Key
# Получить: https://brave.com/search/api/
# Бесплатный tier: 2000 запросов/месяц
BRAVE_SEARCH_API_KEY=your_brave_search_api_key_here

# Next.js Configuration
# URL приложения (для production заменить на реальный домен)
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Optional: Model Configuration
# По умолчанию используется Claude Sonnet 4.5 (последняя версия)
# MODEL_NAME=claude-sonnet-4-5-20250929
